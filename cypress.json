{
  "projectId": "ac3ase",
  "baseUrl": "http://localhost:3000",
  "env": {
    "cypress-plugin-snapshots": {
      "imageConfig": {
        "createDiffImage": true,
        "threshold": 0.8,
        "thresholdType": "percent"
      }
    }
  },
  "firefoxGcInterval": {
    "runMode": null,
    "openMode": null
  },
  "ignoreTestFiles": ["**/__snapshots__/*", "**/__image_snapshots__/*", "*.js", "*.md"],
  "testFiles": "**/*.feature"
}
